# Development Environment Variables
# Copy this file to .env.local and fill in your values

# N8n Configuration
NEXT_PUBLIC_N8N_BASE_URL=your-n8n-instance-url
NEXT_PUBLIC_N8N_SCHEDULE_ENDPOINT=/webhook/ryanair-schedule
NEXT_PUBLIC_N8N_TASKS_ENDPOINT=/webhook/ryanair-tasks

# Authentication Secret (use a strong password)
NEXT_PUBLIC_APP_SECRET=your-secret-password-here

# Skip environment validation during build (set to true for CI/CD)
SKIP_ENV_VALIDATION=false
